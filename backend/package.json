{
  "name": "popular-repos-analyzer",
  "version": "1.0.0",
  "description": "An API for analyzing popular GitHub repositories using LLM",
  "main": "dist/index.js",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler publish",
    "build": "webpack",
    "test": "jest --verbose --runInBand",
    "test:watch": "jest --watch --verbose --runInBand",
    "test:coverage": "jest --coverage --verbose --runInBand",
    "test:verbose": "jest --verbose --runInBand --no-cache",
    "test:fast": "jest",
    "test:silent": "jest --silent",
    "test:index": "jest --verbose --runInBand index.test.ts",
    "test:service": "jest --verbose --runInBand service.test.ts",
    "test:transformer": "jest --verbose --runInBand transformer.test.ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250523.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.5.0",
    "typescript": "^5.8.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}